<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".InputActivity">

    <View
        android:id="@+id/navbar3"
        android:layout_width="422dp"
        android:layout_height="57dp"
        android:layout_marginBottom="4dp"
        android:background="#FFFFFBFE"
        android:outlineAmbientShadowColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.545"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageButton
        android:id="@+id/homebtn"
        android:layout_width="74dp"
        android:layout_height="60dp"
        android:layout_marginStart="28dp"
        android:backgroundTint="#FBFBFB"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/navbar3"
        app:layout_constraintTop_toTopOf="@+id/navbar3"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/home" />

    <ImageButton
        android:id="@+id/inputbtn"
        android:layout_width="74dp"
        android:layout_height="60dp"
        android:backgroundTint="#FBFBFB"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/recordsbtn"
        app:layout_constraintStart_toEndOf="@+id/homebtn"
        app:srcCompat="@drawable/edit" />

    <ImageButton
        android:id="@+id/statbtn"
        android:layout_width="74dp"
        android:layout_height="60dp"
        android:layout_marginEnd="16dp"
        android:backgroundTint="#FBFBFB"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/trend" />

    <ImageButton
        android:id="@+id/recordsbtn"
        android:layout_width="74dp"
        android:layout_height="60dp"
        android:layout_marginEnd="24dp"
        android:backgroundTint="#FBFBFB"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/statbtn"
        app:srcCompat="@drawable/database" />

    <com.google.android.material.slider.Slider
        android:id="@+id/slider1"
        android:layout_width="358dp"
        android:layout_height="114dp"
        android:value="20"
        android:valueFrom="0"
        android:valueTo="100"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.698"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.822" />

    <com.google.android.material.slider.Slider
        android:id="@+id/slider3"
        android:layout_width="358dp"
        android:layout_height="114dp"
        android:value="20"
        android:valueFrom="0"
        android:valueTo="100"
        app:haloColor="@color/colorAccent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.698"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.614"
        app:thumbColor="@color/colorAccent"
        app:tickColor="@color/colorAccent"
        app:trackColor="@color/colorAccent" />

    <com.google.android.material.slider.Slider
        android:id="@+id/HeartRateslider"
        android:layout_width="358dp"
        android:layout_height="114dp"
        android:value="30"
        android:valueFrom="0"
        android:valueTo="150"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.698"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.431" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="462dp"
        android:layout_height="249dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.509"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/up" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="343dp"
        android:layout_height="229dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.638"
        app:layout_constraintStart_toStartOf="@+id/imageView4"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/updoo" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintBottom_toTopOf="@+id/HeartRateslider"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView6"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/ellipse5" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        app:layout_constraintBottom_toTopOf="@+id/slider1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/slider3"
        app:layout_constraintVertical_bias="0.484"
        app:srcCompat="@drawable/ellipse6" />

    <TextView
        android:id="@+id/hrtext3"
        android:layout_width="63dp"
        android:layout_height="49dp"
        android:layout_marginStart="16dp"
        android:drawableTint="#D15B5B"
        android:text="20"
        android:textSize="30dp"
        app:layout_constraintBottom_toTopOf="@+id/HeartRateslider"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.951"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView6"
        app:layout_constraintVertical_bias="0.47" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        app:layout_constraintBottom_toTopOf="@+id/slider3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/HeartRateslider"
        app:layout_constraintVertical_bias="0.511"
        app:srcCompat="@drawable/ellipse3" />

    <TextView
        android:id="@+id/hrtext2"
        android:layout_width="63dp"
        android:layout_height="49dp"
        android:layout_marginEnd="4dp"
        android:drawableTint="#D15B5B"
        android:text="20"
        android:textSize="30dp"
        app:layout_constraintBottom_toTopOf="@+id/slider3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/imageView8"
        app:layout_constraintTop_toBottomOf="@+id/HeartRateslider"
        app:layout_constraintVertical_bias="0.517" />

    <TextView
        android:id="@+id/hrtext"
        android:layout_width="48dp"
        android:layout_height="35dp"
        android:layout_marginTop="44dp"
        android:layout_marginEnd="4dp"
        android:drawableTint="#D15B5B"
        android:text="20"
        android:textSize="30dp"
        app:layout_constraintEnd_toEndOf="@+id/imageView7"
        app:layout_constraintTop_toBottomOf="@+id/slider3" />

    <Button
        android:id="@+id/updtbtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="16dp"
        android:text="Update"
        app:layout_constraintBottom_toTopOf="@+id/navbar3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent" />

    <FrameLayout
        android:id="@+id/flFragment"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

    </FrameLayout>

</androidx.constraintlayout.widget.ConstraintLayout>